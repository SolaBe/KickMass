<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sola2be.kickmass">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ListFollowersActivity"
            android:label="@string/title_activity_list_followers"
            android:theme="@style/AppTheme.NoActionBar"></activity>


        <meta-data
            android:name="com.instagram.instagramapi.InstagramAppClientId"
            android:value="eacc077eee2d4fa685f1d75face80c72" />

        <meta-data
            android:name="com.instagram.instagramapi.InstagramAppRedirectURL"
            android:value="http://sitedeveloper.com.ua/" />
    </application>

</manifest>
